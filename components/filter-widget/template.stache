<div class="filter-widget">
  <panel-container collapsible="true" heading="Filter" open="false">
    <h4>Add Filter</h4>
    <form-widget inline="true" {fields}="fields" {form-object}="formObject" (submit)="addFilter" />
    <h4>Current Filters</h4>
    <list-table {objects}="filters" (delete)="removeFilter" {buttons}="buttons" />
  </panel-container>
</div>

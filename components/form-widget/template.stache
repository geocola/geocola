<form class="form{{#inline}}-inline{{/inline}}" ($submit)="submitForm">
  {{#each _fieldObjects}}
    {{>template}}
  {{/each}}

  {{#if showButtons}}
    <button type="button" class="btn btn-default" ($click)="submitForm">Submit</button>
    <a class="btn btn-default" ($click)="cancelForm">Cancel</a>
  {{/if}}
</form>

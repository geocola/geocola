<form class="form{{#inline}}-inline{{/inline}}" ($submit)="formSubmit">
  {{#each formObject}}
    <div class="form-group">
      <label for="{{%key}}">{{formatField(%key)}}</label>
      <input type="text" class="form-control" id="{{%key}}" name="{{%key}}" placeholder="" value="{{.}}">
    </div>
  {{/each}}

  <button type="submit" class="btn btn-default">Submit</button>
  <a class="btn btn-default" ($click)="cancelForm">Cancel</a>
</form>

<table class="table table-striped table-condensed">
  <tbody>
    <tr>
      <td colspan="2">
        <img src="http://www.worldlicenseplates.com/flags/US_{{attributes.STATE_ABBR.rawValue}}XX.gif" style="width:100%;" />
      </td>
    </tr>
    {{#each attributes}}
      <tr>
        <td>{{alias}}</td>
        <td>{{value}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

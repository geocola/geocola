{
  "name": "GeoCola",
  "version": "0.8.0",
  "description": "A configureable openlayers mapping and data management app",
  "repository": "https://github.com/roemhildtg/geocola",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "steal-tools",
    "watch": "npm-watch",
    "docs": "documentjs --watch -d",
    "export": "node build.js",
    "lint": "jshint .",
    "pretest": "npm run lint"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "documentjs": "^0.4.2",
    "jshint": "latest",
    "npm-watch": "^0.1.4",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.16.2",
    "testee": "^0.2.5"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "can": "^2.3.23",
    "can-connect": "^0.5.4",
    "can-set": "^0.6.0-pre.3",
    "date-selector": "^2.0.0",
    "font-awesome": "^4.5.0",
    "can-crud": "git+https://github.com/roemhildtg/can-crud.git",
    "can-geo": "git+https://github.com/roemhildtg/can-geo.git",
    "can-ui": "git+https://github.com/roemhildtg/can-ui.git",
    "jquery": "^2.2.3",
    "openlayers": "^3.15.1",
    "proj4": "^2.3.14",
    "pubsub-js": "^1.5.3",
    "steal": "^0.16.4",
    "underscore": "^1.8.3"
  },
  "main": "loader",
  "system": {
    "bundleAssets": true,
    "bundle": [
      "app/viewer/viewer",
      "app/viewer/config/default/default",
      "app/crud/crud",
      "app/crud/config/default/default"
    ],
    "npmIgnore": [
      "openlayers",
      "documentjs",
      "jshint",
      "steal-tools",
      "testee"
    ],
    "paths": {
      "bootstrap": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    },
    "shim": {
      "jquery": {
        "exports": "jQuery"
      },
      "bootstrap": {
        "deps": [
          "jquery"
        ]
      }
    }
  }
}

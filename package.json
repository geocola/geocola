{
  "name": "GeoCola",
  "version": "0.8.0",
  "description": "A configureable openlayers mapping and data management app",
  "repository": "https://github.com/roemhildtg/geocola",
  "watch": {
    "lint": "**/*.js",
    "test": "**/*.js"
  },
  "scripts": {
    "test": "testee test/index.html --reporter Spec --browsers firefox",
    "build": "steal-tools",
    "watch": "npm-watch",
    "docs": "documentjs --watch -d",
    "export": "node build.js",
    "lint": "jshint .",
    "pretest": "npm run lint"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.10.2",
    "npm-watch": "^0.1.4",
    "steal-tools": "^0.16.8",
    "testee": "^0.2.5"
  },
  "dependencies": {
    "can-define": "^1.0.4",
    "can-geo": "github:roemhildtg/can-geo",
    "can-route": "^3.0.4",
    "can-stache": "^3.0.8",
    "font-awesome": "^4.7.0",
    "openlayers": "^3.19.1",
    "proj4": "^2.3.15",
    "sidebar-v2": "github:turbo87/sidebar-v2#0cd125b4eae37f87d753c5d6f99e613a7d88ea4f",
    "spectre.css": "^0.1.27",
    "steal": "^0.16.42",
    "steal-stache": "^3.0.1"
  },
  "main": "app",
  "system": {
    "npmAlgorithm": "flat",
    "npmIgnore": [
      "babel-eslint",
      "eslint",
      "npm-watch",
      "steal-tools",
      "testee"
    ],
    "bundleAssets": true,
    "bundle": [],
    "paths": {
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    }
  }
}

{
  "name": "cola",
  "version": "1.0.0",
  "description": "a modular canjs/openlayers app",
  "repository": "",
  "scripts": {
    "test": "test",
    "build": "steal-tools",
    "watch": "steal-tools --watch",
    "docs": "documentjs --watch",
    "export": "steal-tools transform app.js"
  },
  "author": "roemhildtg",
  "license": "MIT",
  "devDependencies": {
    "documentjs": "^0.4.2",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.15.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "can": "^2.3.15",
    "can-connect": "^0.3.4",
    "font-awesome": "^4.4.0",
    "openlayers": "^3.12.0",
    "proj4": "^2.3.12",
    "steal": "^0.15.0"
  },
  "main": "cola",
  "system": {
    "bundle": [
      "app/viewer/",
      "app/viewer/config/default/",
      "app/locator/"
    ],
    "npmIgnore": [
      "openlayers"
    ],
    "paths": {
      "openlayers": "node_modules/openlayers/dist/ol-debug.js"
    },
    "meta": {
      "openlayers": {
        "format": "global",
        "exports": "ol"
      }
    },
    "shim": {
      "jquery": {
        "exports": "jQuery"
      },
      "bootstrap": {
        "deps": [
          "jquery"
        ]
      }
    }
  }
}
